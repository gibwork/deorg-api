{
  "name": "gibwork-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --detectOpenHandles --runInBand --logHeapUsage",
    "test:watch": "jest --watch",
    "test:cov": "jest --detectOpenHandles --runInBand --logHeapUsage --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/core/infra/database/database.ts",
    "migration:create": "yarn typeorm migration:create src/core/infra/database/migrations/rename_migration",
    "migration:generate": "npm run typeorm:cli migration:generate src/core/infra/database/migrations/rename_migration",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:run -d ./src/core/infra/database/database.ts",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:revert -d ./src/core/infra/database/database.ts"
  },
  "resolutions": {
    "@octokit/rest": "19.0.11"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.650.0",
    "@clerk/backend": "^1.13.8",
    "@clerk/express": "^1.3.32",
    "@coral-xyz/anchor": "^0.31.1",
    "@metaplex-foundation/js": "^0.20.1",
    "@nestjs/cache-manager": "^2.2.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^11.0.14",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/websockets": "^11.0.14",
    "@octokit/rest": "19.0.11",
    "@solana/spl-token": "^0.4.8",
    "@solana/web3.js": "^1.95.3",
    "@ssut/nestjs-sqs": "^3.0.0",
    "@types/cls-hooked": "^4.3.9",
    "@types/jsonwebtoken": "^9.0.6",
    "@willsoto/nestjs-prometheus": "^6.0.1",
    "@xcrowdev/node": "1.10.0",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.6.8",
    "axios-retry": "^4.5.0",
    "bignumber.js": "^9.1.2",
    "bs58": "^6.0.0",
    "cache-manager": "^4.1.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transform": "^0.7.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cls-hooked": "^4.2.2",
    "colorette": "^2.0.20",
    "dotenv": "^16.4.5",
    "express-rate-limit": "^7.4.0",
    "ioredis": "^5.3.1",
    "jsonwebtoken": "^9.0.2",
    "nest-winston": "^1.9.4",
    "pg": "^8.11.5",
    "prom-client": "^15.1.3",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.0",
    "response-time": "^2.3.2",
    "rxjs": "^7.8.1",
    "sanitize-html": "^2.13.0",
    "slugify": "^1.6.6",
    "socket.io": "^4.8.1",
    "svix": "^1.24.0",
    "tweetnacl": "^1.0.3",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^11.1.0",
    "winston": "^3.11.0",
    "winston-transport": "^4.6.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.4.5",
    "@types/cache-manager": "^4.0.2",
    "@types/cron": "^2.4.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/response-time": "^2.3.8",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.1.6",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": "/test/.*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {}
      ]
    },
    "moduleNameMapper": {
      "^@core/(.*)$": "<rootDir>/src/core/$1",
      "^@domains/(.*)$": "<rootDir>/src/domains/$1",
      "^@utils/(.*)$": "<rootDir>/src/utils/$1",
      "^@mocks/(.*)$": "<rootDir>/test/__mocks__/$1"
    },
    "collectCoverageFrom": [
      "src/domains/**/*.service.ts",
      "src/domains/**/*.usecase.ts",
      "src/domains/**/*.listener.ts",
      "src/domains/**/*.scheduler.ts",
      "src/utils/**/*.ts"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 16,
        "statements": 15,
        "branches": 14,
        "functions": 10
      }
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
